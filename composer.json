    "keywords": [
        "Snapchat",
        "SnapAPI"
    ],
    "authors": [
        {
            "role": "Contributors",
            "name": "Snap API Contributing Team",
            "homepage": "https://github.com/mgp25/Snap-API/graphs/contributors"
        }
